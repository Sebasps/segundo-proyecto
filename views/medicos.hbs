<main>
    <h2>Listado de médicos</h2>
    <table>
        <thead>
            <tr>
                <th>Nombres</th>
                <th>Apellidos</th>
                <th>Número de Cédula</th>
                <th>Número de consultorio</th>
                <th>Telefono</th>
                <th>Correo</th>
                <th>Especialidad</th>
            </tr>
        </thead>
        <tbody>
            {{!-- Tabla dinamica --}}
            {{#each resultado}}
            <tr>
                <td>{{nombres}}</td>
                <td>{{apellidos}}</td>
                <td>{{cedula}}</td>
                <td>{{consultorio}}</td>
                <td>{{telefono}}</td>
                <td>{{correo}}</td>
                <td>{{especialidad}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</main>
